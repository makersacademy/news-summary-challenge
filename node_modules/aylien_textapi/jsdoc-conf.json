{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["textapi.js", "lib/"],
    "includePattern": ".+\\.js$"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  }
}
